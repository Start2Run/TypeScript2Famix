    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@ts-morph/bootstrap": "^0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Start2Run/TypeScript2Famix.git"
  },
  "bugs": {
